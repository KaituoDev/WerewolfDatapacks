data modify block 0 0 0 Items[0].tag.pages append value '[{"text": "<怨靈陣營>", "color": "red", "bold": true}, {"text": "  [怨靈]", "color": "dark_blue", "bold": true}, {"text": "\\n"}, {"text": "[ 蠢 ]\\n", "color": "aqua", "bold": true, "hoverEvent": {"action": "show_text", "contents": [{"text": "點我", "color": "white", "italic": false}]}, "clickEvent": {"action": "change_page", "value": "47"}}, {"text": "註1", "color": "dark_aqua", "bold": true, "hoverEvent": {"action": "show_text", "contents": [{"text": "當 <狼人陣營> 全滅, 場上尚有 <怨靈陣營> 存活時, <怨靈陣營> 獲勝", "color": "white", "italic": false}]}}, {"text": "\\n"}, {"text": "\\n[ 暴民式 ]:\\n", "color": "black"}, {"text": "  怨靈被動", "color": "dark_aqua", "bold": true, "hoverEvent": {"action": "show_text", "contents": [{"text": "場上尚有<好人陣營>玩家存活時, 晚上時永久無敵, 唯有被預言家查驗時會死或白天被票死", "color": "white", "italic": false}]}}, {"text": "\\n"}, {"text": "  隱形/現形", "color": "dark_aqua", "bold": true, "hoverEvent": {"action": "show_text", "contents": [{"text": "(夜晚限定) 可自由切換隱形效果, 無敵不受影響", "color": "white", "italic": false}]}}, {"text": "\\n"}, {"text": "\\n[ 辯論式 ]:\\n", "color": "black"}, {"text": "  怨靈被動", "color": "dark_aqua", "bold": true, "hoverEvent": {"action": "show_text", "contents": [{"text": "場上尚有<好人陣營>玩家存活時, 晚上無法被任何手段殺死, 唯有被預言家查驗時死或白天被票死", "color": "white", "italic": false}]}}, {"text": "\\n"}]'
